class Solution:
    def isPalindrome(self, x: int) -> bool:
        # arr = list(str(x))
        # rev = arr[::-1]
        # if arr == rev:
        #     return True
        # else :
        #     return False
        return str(x) == str(x)[::-1]

            
# time complexity : O(n) 
# space complexity : O(n)
